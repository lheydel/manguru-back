{
  "name": "manguru",
  "version": "0.0.1",
  "description": "impossiburuuu",
  "repository": {
    "type": "git",
    "url": "https://github.com/lheydel/manguru-back.git"
  },
  "author": "Luc Heydel",
  "license": "ISC",
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-ts && npm run tslint",
    "build-ts": "tsc",
    "debug": "echo \"npm run build\" && npm run watch-debug",
    "serve": "node dist/server.js",
    "serve-debug": "nodemon --inspect=0.0.0.0:5858 dist/server.js",
    "test": "echo \"jest --forceExit --coverage --verbose\" && exit 0",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Typescript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"Typescript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "watch-node": "nodemon dist/server.js",
    "watch-ts": "tsc -w"
  },
  "dependencies": {
    "bluebird": "^3.5.5",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "express": "^4.16.4",
    "mongoose": "^5.6.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.27",
    "@types/body-parser": "^1.17.0",
    "@types/compression": "0.0.36",
    "@types/express": "^4.11.1",
    "@types/mongodb": "^3.1.28",
    "@types/mongoose": "^5.5.6",
    "@types/node": "^11.13.4",
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.11",
    "ts-node": "^5.0.0",
    "tslint": "^5.9.1",
    "typegoose": "^5.7.2",
    "typescript": "^3.4.3",
    "typescript-ioc": "^1.2.5"
  }
}
